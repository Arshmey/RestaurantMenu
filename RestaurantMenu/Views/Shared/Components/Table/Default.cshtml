@model IEnumerable<Dish>

<table class="table table-striped">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DateCreate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Composition)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grams)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Calorie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CookTime)
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (Dish item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(model => item.DateCreate)
            </td>
            <td>
                @Html.DisplayFor(model => item.Name)
            </td>
            <td>
                @Html.DisplayFor(model => item.Composition)
            </td>
            <td>
                @Html.DisplayFor(model => item.Description)
            </td>
            <td>
                @Html.DisplayFor(model => item.Price)
            </td>
            <td>
                @Html.DisplayFor(model => item.Grams)
            </td>
            <td>
                @Html.DisplayFor(model => item.Calorie)
            </td>
            <td>
                @Html.DisplayFor(model => item.CookTime)
            </td>
        </tr>
    }
    </tbody>
</table>